<Window x:Class="KosmicznaPodroz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KosmicznaPodroz"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Kosmiczna Podróż" Height="500" Width="800">
    <Grid>
        <local:StronaStartowa x:Name="stronaStartowa" />
        <local:StronaInstrukcji x:Name="stronaPomoc" Visibility="Hidden" />
        <local:StronaOProgramie x:Name="stronaOProgramie" Visibility="Hidden" />
        <local:StronaSymulacji x:Name="stronaSymulacji" Visibility="Hidden" />
    </Grid>
</Window>
